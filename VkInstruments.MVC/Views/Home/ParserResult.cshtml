@{
    ViewBag.Title = "VK like parser";
}

<h1 class="h1">Результат парсинга лайков с поста: </h1>

<textarea style="margin-top: 10px; width: 25%; max-width: 500px">
 @{
     foreach (var likeId in @ViewBag.LikeIds)
     {
         var adress = $"vk.com/id{likeId}\n";
         @adress
     }
 }
</textarea>