<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - VkInstruments</title>
    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.3.1.min.js"></script>
    @Styles.Render("~/Content/css")
</head>
<body>
    <header class="blog-masthead">
        <nav class="navbar">
            <div class="container">
                <div class="left-nav">
                </div>
                <div class="right-nav">
                    <a class="right-title">VK Instruments</a>
                </div>
            </div>
        </nav>
    </header>
    <div class="container body-content">
        @RenderBody()
        <div style="padding-top: 100px" />
        <footer class="modal-footer">
            <p style="font-size: smaller">&copy; @DateTime.Now.Year - VkInstruments</p>
        </footer>
    </div>
</body>
</html>     